{
    "name": "backstageel/blade-bootstrap-components",
    "description": "A set of Blade Components based on Boostrap-4 CSS Framework",
    "keywords": [
        "backstageel",
        "blade-bootstrap-components"
    ],
    "homepage": "https://github.com/backstageel/blade-bootstrap-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ElÃ­sio Leonardo",
            "email": "elisio.leonardo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": ">7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": ">8.0"
    },
    "autoload": {
        "psr-4": {
            "Backstageel\\BladeBootstrapComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backstageel\\BladeBootstrapComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backstageel\\BladeBootstrapComponents\\BladeBootstrapComponentsServiceProvider"
            ],
            "aliases": {
                "BladeBootstrapComponents": "Backstageel\\BladeBootstrapComponents\\BladeBootstrapComponentsFacade"
            }
        }
    }
}
